bracketSpacing: true
jsxBracketSameLine: false
printWidth: 100
semi: true
singleQuote: true
tabWidth: 4
trailingComma: es5
useTabs: true
overrides:
  -
    files: '{.watchmanconfig,.ember-cli,config/*.json}'
    options:
      parser: json
  -
    files: '*.yaml'
    options:
      parser: yaml
      useTabs: false
      tabWidth: 2
